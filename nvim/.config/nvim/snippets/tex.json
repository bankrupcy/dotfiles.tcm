{
  "Template": {
    "prefix": "<template",
    "body": [
      "\\documentclass[a4paper,10pt]{article}",
      "\\usepackage{fancyhdr}",
      "\\usepackage{lastpage}",
      "\\usepackage{geometry}",
      "\\usepackage{setspace}",
      "\\usepackage{fontspec}",
      "\\usepackage{graphicx}",
      "\\usepackage{listings}",
      "\\usepackage{booktabs}",
      "\\usepackage{amsmath,mathrsfs,amsfonts}",
      "\\usepackage{pgfplots}",
      "\\usepackage{tikz}",
      "",
      "\\usetikzlibrary{graphs,graphdrawing,arrows.meta}",
      "\\usegdlibrary{trees}",
      "\\setstretch{1.25}",
      "\\setlength{\\parindent}{0pt}",
      "",
      "\\pagestyle{fancy}",
      "\\fancyhf{}",
      "",
      "\\renewcommand\\footrule{\\begin{minipage}{1\\textwidth}",
      "		\\hrule width \\hsize height 2pt \\kern 1mm \\hrule width \\hsize",
      "	\\end{minipage}\\par}%",
      "",
      "\\renewcommand\\headrule{",
      "	\\begin{minipage}{1\\textwidth}",
      "		\\hrule width \\hsize \\kern 1mm \\hrule width \\hsize height 2pt",
      "	\\end{minipage}}%",
      "",
      "\\lhead{}",
      "\\rhead{Michael Bao}",
      "\\lfoot{Jun, 2023}",
      "\\rfoot{Page \\thepage\\ of \\ \\pageref{LastPage}}",
      "",
      "\\begin{document}",
      "",
      "\\end{document}"
    ]
  },
  "Begin Align*": {
    "prefix": "ba",
    "body": ["\\begin{align*}", "    $0", "\\end{align*}"]
  },
  "Summation": {
    "prefix": "sum",
    "body": ["\\sum_{${1:i=1}}^{${2:n}} $0"]
  }
}
